{
  "expo": {   
    "name": "ePharmacyLocator",
    "slug": "ePharmacyLocator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/spinnerLogo.png",
    "scheme": "ePharmacyLocator",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/spinnerLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/spinnerLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ePharmacyLocator.com",
      "newArchEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB21rjoBoJCLlsRUjjKd-8WihrlBO7SLOk"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/spinnerLogo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}